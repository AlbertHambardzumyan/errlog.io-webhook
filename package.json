{
  "name": "errlog.io-webhook",
  "version": "1.0.0",
  "description": "NodeJS module for the errlog.io webhook",
  "scripts": {
    "build": "rm -rf dist/* && npm run compile --outDir dist",
    "compile": "tsc --project src/",
    "compile:clean": "rm -rf src/*.js",
    "compile:watch": "tsc --watch --project src/",
    "test": "npm run test:once",
    "test:watch": "npm run compile:clean && concurrently --allow-restart --success first 'npm run compile:watch' 'echo $(ls src)' 'sleep 1 && mocha --timeout 10000 --watch src/**/*.spec.js'",
    "test:once": "mocha src/**/*.spec.js"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/intribeco/errlog.io-webhook.git"
  },
  "keywords": [
    "errlog.io",
    "error",
    "logging",
    "monitoring"
  ],
  "author": "Aaron Chilcott <aaron@intribe.co>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/intribeco/errlog.io-webhook/issues"
  },
  "homepage": "https://github.com/intribeco/errlog.io-webhook#readme",
  "dependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.48",
    "@types/request-promise": "^4.1.41",
    "@types/request-promise-native": "^1.0.13",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^3.5.1",
    "dateformat": "^3.0.3",
    "mocha": "^5.0.1",
    "request": "^2.83.0",
    "request-promise-any": "^1.0.5",
    "request-promise-native": "^1.0.5",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "typescript-compiler": "^1.4.1-2"
  },
  "devDependencies": {
    "@types/request": "^2.47.0",
    "@types/simple-mock": "0.0.27",
    "simple-mock": "^0.8.0"
  }
}
