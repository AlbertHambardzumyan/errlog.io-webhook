{
  "compilerOptions": {
    "target": "es5",
    "strict": true,
    "declaration":true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [ "es5", "es2015"   ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true
  }
}
